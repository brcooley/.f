[user]
	name = brc
	email = brett@cooleyweb.org
[branch "master"]
	remote = origin
	merge = refs/heads/master
[remote "origin"]
	fetch = +refs/heads/*:refs/remotes/origin/*
[color]
	ui = auto
[color "status"]
	changed = yellow
[color "branch"]
	current = reverse
	remote = magenta
[alias]
	br = branch -a
	l = log --oneline --graph
