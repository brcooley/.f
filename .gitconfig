[core]
	autocrlf = input
	safecrlf = true
[user]
	name = brc
	email = brett@cooleyweb.org
[branch "master"]
	remote = origin
	merge = refs/heads/master
[color]
	ui = auto
[color "status"]
	changed = yellow
[color "branch"]
	current = reverse
	remote = magenta
[alias]
	br = branch -a
	l = log --graph --pretty=format:'%C(yellow)%h%Creset %s [%ar]'
